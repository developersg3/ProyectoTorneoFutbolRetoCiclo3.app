@page
@using TorneoFutbolRetoCiclo3.App.Dominio
@model TorneoFutbolRetoCiclo3.App.Frontend.Pages.Municipios.CreateModel
@{
}
<h1>Nuevo Municipio</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="municipio.nombre_municipio"></label>
        <input asp-for="municipio.nombre_municipio" class="form-control">
        <span asp-validation-for="municipio.nombre_municipio" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Crear Municipio</button>
</form>
<a class="btn btn-secondary" asp-page="./Index">Volver</a>
