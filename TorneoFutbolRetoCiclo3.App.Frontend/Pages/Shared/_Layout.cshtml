                            <a class="nav-link text-dark" asp-area="" asp-page="/Equipos/Index">Equipos</a>
                        </li>


                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Municipios/Index">Municipios</a>
                        </li>

<!--

                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Estadios/Index">Estadios</a>
                        </li>
-->


                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Posicion_jugadores/Index">Posiciones</a>
                        </li>

<!--

                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Tecnicos/Index">Tecnicos</a>
                        </li>
-->
<!--

                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Tipo_novedades/Index">Tipo Novedades</a>
                        </li>
-->

                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Jugadores/Index">Jugadores</a>
                        </li>
<!--

                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Desempeno_equipos/Index">Desempeño Equipos</a>
                        </li>
-->
<!--

                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Partidos/Index">Partidos</a>
                        </li>
-->
<!--

                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Novedades/Index">Novedades</a>
                        </li>
-->
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
<!--
    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - TorneoFutbolRetoCiclo3.App.Frontend - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>
-->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
